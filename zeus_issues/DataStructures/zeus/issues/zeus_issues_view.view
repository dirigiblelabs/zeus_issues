{
  "viewName":"zeus_issues_view",
  "query":"SELECT ISSUE_ID, ISSUE_NAME, ISSUE_DESCRIPTION, ISSUE_AUTHOR, ISSUE_DATE, 
  ISSUE_ASSIGNEE, ISSUE_PROJECT_ID, ISSUE_ISTATUS_ID, PROJECT_NAME, ISTATUS_NAME 
FROM ZEUS_ISSUES, ZEUS_PROJECTS, ZEUS_ISSUE_STATUS
WHERE
  ISSUE_PROJECT_ID = PROJECT_ID
  AND ISSUE_ISTATUS_ID = ISTATUS_ID"
}